DASM 	= ~aycock/599.82/bin/dasm

%.prg: 
	$(DASM) $< -o$@ -l$*.lst
	cp $@ ~/www/asdwf$@
	chmod 644 ~/www/asdwf$@
	echo https://pages.cpsc.ucalgary.ca/~emmett.collings1/asdwf$@

all: ogtest.prg betterogtest.prg

clean: 
	rm -rf *.lst *.prg

ogtest.prg: originalTitleScreen.s
betterogtest.prg: betterOGTitle.s

